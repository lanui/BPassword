{
  "applications": {
    "gecko": {
      "strict_min_version": "78.0"
    }
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
  "permissions": [
    "<all_urls>",
    "tabs",
    "activeTab",
    "storage",
    "unlimitedStorage",
    "notifications",
    "idle",
    "privacy"
  ]
}
