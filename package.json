{
  "name": "bpassword",
  "version": "0.7.2",
  "description": "BPassword management your certificates",
  "author": "lanbery",
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "prettier": "prettier \"src/**/*.{js,vue}\"",
    "prettier:write": "npm run prettier -- --write",
    "build": "cross-env NODE_ENV=production webpack --config ci/webpack.config.js --hide-modules",
    "build:dev": "cross-env NODE_ENV=development webpack --config ci/webpack.config.js --hide-modules",
    "build-zip": "node ci/build-zip.js",
    "watch": "cross-env HMR=true npm run build -- --watch",
    "init:dev": "node ci/init-development-env.js",
    "watch:prod": "cross-env APP_VERSION=2.0.0 HMR=true npm run build:dev -- ",
    "build-pre": "cross-env HMR=true NODE_ENV=development  webpack --config ci/webpack.config.js --hide-modules",
    "pre:watch": "cross-env HMR=true npm run build-pre -- --watch",
    "build:crx": "cross-env EXT_TARGET=chrome webpack --config ci/webpack.crx.js --hide-modules",
    "watch:crx": "cross-env HMR=true LOG_LEVEL=DEBUG npm run build:crx -- --watch",
    "build:fox": "cross-env EXT_TARGET=firefox webpack --config ci/webpack.fox.js --hide-modules",
    "build:fox-rel": "cross-env LOG_LEVEL=WARN EXT_TARGET=firefox webpack --config ci/webpack.fox.js --hide-modules",
    "watch:fox": "cross-env HMR=true NODE_ENV=development LOG_LEVEL=DEBUG npm run build:fox -- --watch",
    "zip-fox": "cross-env node ci/zip-fox.js",
    "ext:build": "cross-env TASK_COMMAND=build node ci/webext-distribution.js",
    "ext:sign-pre": "cross-env TASK_COMMAND=sign node ci/webext-distribution.js",
    "ext:sign-rel": "cross-env TASK_COMMAND=sign $WEB_EXT_CHANNEL=listed node ci/webext-distribution.js",
    "analyz": "webpack-bundle-analyzer dist/stats.json --port 8899",
    "docs": "docsify serve docs"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@mdi/font": "^5.8.55",
    "@webcomponents/custom-elements": "^1.4.3",
    "aes-js": "^3.1.2",
    "await-semaphore": "^0.1.3",
    "axios": "^0.19.0",
    "bs58": "^4.0.1",
    "buffer": "^6.0.0",
    "clipboard": "^2.0.6",
    "debounce-stream": "^2.0.0",
    "end-of-stream": "^1.4.4",
    "eth-sig-util": "^2.5.3",
    "extension-port-stream": "^1.0.0",
    "keythereum": "^1.2.0",
    "lodash": "^4.17.20",
    "loglevel": "^1.7.0",
    "nanoid": "^3.1.16",
    "obj-multiplex": "^1.0.0",
    "obs-store": "^4.0.3",
    "post-message-stream": "^3.0.0",
    "promise-to-callback": "^1.0.0",
    "pump": "^3.0.0",
    "qrcode": "^1.4.4",
    "readable-stream": "^3.6.0",
    "resize-observer-polyfill": "^1.5.1",
    "tweetnacl": "^1.0.3",
    "uuid": "^8.3.1",
    "vue": "^2.6.10",
    "vue-i18n": "^8.22.1",
    "vue-router": "^3.4.7",
    "vuetify": "^2.3.9",
    "vuex": "^3.5.1",
    "web3-utils": "^1.3.0",
    "webextension-polyfill": "^0.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/runtime-corejs3": "^7.12.1",
    "archiver": "^5.0.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.1.0",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.2.1",
    "core-js": "^3.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "3.4.0",
    "deepmerge": "^4.2.2",
    "ejs": "^3.1.5",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.0.1",
    "fibers": "^5.0.0",
    "file-loader": "^5.0.2",
    "fs-extra": "^9.0.1",
    "husky": "^2.4.0",
    "mini-css-extract-plugin": "0.10.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "sass": "^1.27.0",
    "sass-loader": "9.0.3",
    "url-loader": "^4.1.1",
    "vue-loader": "^15.9.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "vuetify-loader": "^1.6.0",
    "web-ext": "^5.4.0",
    "web-ext-types": "^3.2.1",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^4.1.0",
    "webpack-cli": "^3.3.12",
    "webpack-extension-reloader": "^1.1.4",
    "webpack-merge": "4.2.2"
  }
}
